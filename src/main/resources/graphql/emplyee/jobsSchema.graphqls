type Jobs {
    id: ID!
    title: String!
    minSalary: Float
    maxSalary: Float
}

type Query {
    getAllJobs: [Jobs]
    getJobById(id: ID!): Jobs
    findJobByTitle(title: String!): [Jobs]
}

type Mutation {
    createJob(title: String!, minSalary: Float, maxSalary: Float) : Jobs
    updateJob(id: ID!, title: String, minSalary: Float, maxSalary:Float): Jobs
    deleteJob(id: ID!): Boolean
}